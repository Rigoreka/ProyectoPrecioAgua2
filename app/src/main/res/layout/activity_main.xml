<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#40ADE5"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/Titulotxt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="14dp"
        android:padding="15dp"
        android:text="Precios promedio del agua potable"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="14dp"
        android:padding="15dp"
        android:text="Periodo 1994 - 2016"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Titulotxt" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="14dp"
        android:padding="15dp"
        android:text="Periodo 2017 - 2022"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPeriodo" />

    <Button
        android:id="@+id/btnPeriodo"
        android:layout_width="197dp"
        android:layout_height="50dp"
        android:layout_margin="15dp"
        android:layout_marginStart="107dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="199dp"
        android:backgroundTint="@color/white"
        android:text="Mostrar"
        android:textColor="#113049"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/btn2017"
        android:layout_width="150dp"
        android:layout_height="55dp"
        android:layout_margin="15dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="144dp"
        android:backgroundTint="@color/white"
        android:text="2017"
        android:textColor="#113049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/btn2018"
        android:layout_width="150dp"
        android:layout_height="58dp"
        android:layout_margin="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="12dp"
        android:backgroundTint="@color/white"
        android:text="2018"
        android:textColor="#113049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn2017" />

    <Button
        android:id="@+id/btn2019"
        android:layout_width="150dp"
        android:layout_height="55dp"
        android:layout_margin="15dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="@color/white"
        android:text="2019"
        android:textColor="#113049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn2018" />

    <Button
        android:id="@+id/btn2020"
        android:layout_width="150dp"
        android:layout_height="55dp"
        android:layout_margin="15dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="82dp"
        android:backgroundTint="@color/white"
        android:text="2020"
        android:textColor="#113049"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn2017"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/btn2021"
        android:layout_width="150dp"
        android:layout_height="55dp"
        android:layout_margin="15dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="81dp"
        android:backgroundTint="@color/white"
        android:text="2021"
        android:textColor="#113049"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn2018"
        app:layout_constraintTop_toBottomOf="@+id/btn2020" />

    <Button
        android:id="@+id/btn2022"
        android:layout_width="150dp"
        android:layout_height="55dp"
        android:layout_margin="15dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="81dp"
        android:backgroundTint="@color/white"
        android:text="2022"
        android:textColor="#113049"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn2019"
        app:layout_constraintTop_toBottomOf="@+id/btn2021" />
</androidx.constraintlayout.widget.ConstraintLayout>